<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE web-app
    PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
    "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
    <servlet>
        <servlet-name>org.myeducation.portal.MainPage TaskLoad</servlet-name>
        <servlet-class>org.myeducation.portal.server.TaskLoadImpl</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>org.myeducation.portal.MainPage TaskLoad</servlet-name>
        <url-pattern>/Main/TaskLoad</url-pattern>
    </servlet-mapping>

    <!-- Default page to serve -->
  <welcome-file-list>
    <welcome-file>Main.html</welcome-file>
  </welcome-file-list>
</web-app>
